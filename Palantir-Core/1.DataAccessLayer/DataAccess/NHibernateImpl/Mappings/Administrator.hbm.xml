<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Ix.Palantir.DomainModel" namespace="Ix.Palantir.DomainModel">

  <class name="Administrator" table="administrator" lazy="true" select-before-update="false" dynamic-update="false">
    <id name="Id" type="Int32" unsaved-value="0">
      <generator class="sequence">
        <param name="sequence">administrator_id_seq</param>
      </generator>
    </id>

    <property name="UserId" type="Int64" />
    <property name="VkGroupId" column="VkGroupId" />

  </class>

</hibernate-mapping>