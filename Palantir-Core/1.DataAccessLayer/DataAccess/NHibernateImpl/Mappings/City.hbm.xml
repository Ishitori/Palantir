<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Ix.Palantir.DomainModel" namespace="Ix.Palantir.DomainModel">

  <class name="City" table="city" lazy="true" select-before-update="false" dynamic-update="false">
    <id name="Id" type="Int32" unsaved-value="0">
      <generator class="sequence">
        <param name="sequence">city_id_seq</param>
      </generator>
    </id>

    <property name="VkId" type="String" />
    <property name="Title" type="String" />
  </class>

</hibernate-mapping>