@model Ix.Palantir.Admin.UI.Models.UserListModel

@{
    ViewBag.Title = "Список пользователей";
}

<h2>Список пользователей</h2>

<p>
    @Html.ActionLink("Добавить пользователя", "Create", "Users", new { accountId = Model.AccountId }, null)
</p>
<table>
    <tr>
        <th>
            Имя
        </th>
        <th>
            Емейл
        </th>
        <th>
            
        </th>
    </tr>

@foreach (var item in Model.Users) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id })
            <div style="display: none">@Html.ActionLink("Delete", "Delete", new { id = item.Id })</div>
        </td>
    </tr>
}

</table>
